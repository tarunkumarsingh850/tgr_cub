<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="journal_line_import_update_wizard" model="ir.ui.view">
      <field name="name">journal.line.wizard</field>
      <field name="model">journal.line.wizard</field>
      <field name="arch" type="xml">
        <form string="Import Lines">
          <group>
            <field name="excel_file" />
            <field name="journal_line_update_id" invisible="1"/>
          </group>
          <group>
            <button string="Download Template" name="generate_template" type="object"/>
          </group>
          <footer>
            <button name="load_lines" string="Import" type="object" class="btn-primary"/>
            <button string="Cancel" class="btn-default" special="cancel"/>
          </footer>
        </form>
      </field>
    </record>

</odoo>
