<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_quantity_history_inherit_view_form" model="ir.ui.view">
        <field name="name">stock.quantity.history.inherit.view.form</field>
        <field name="model">stock.quantity.history</field>
        <field name="inherit_id" ref="stock.view_stock_quantity_history"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='inventory_datetime']" position="after">
                <field name="location_ids" widget="many2many_tags" attrs="{'invisible': [('inventory_datetime', '=', 0)]}" invisible="1"/>
                <field name="brand_id" attrs="{'invisible': [('inventory_datetime', '=', 0)]}" invisible="1"/>
                <field name="warehouse_stock_id" invisible="1"/>
            </xpath>
        </field>
    </record>

</odoo>
