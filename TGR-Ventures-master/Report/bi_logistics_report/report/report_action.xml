<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_export_report" model="ir.actions.report">
        <field name="name">Logistics Sales Report</field>
        <field name="model">logistics.wizard</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">bi_logistics_report.export_report_xlsx</field>
        <field name="report_file">bi_logistics_report.export_report_xlsx</field>
        <field name="print_report_name">"Logistics Sales Report"</field>
    </record>

    <record id="action_pdf_report" model="ir.actions.report">
        <field name="name">Logistics Sales Report</field>
        <field name="model">logistics.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">bi_logistics_report.logistic_pdf_report</field>
        <field name="report_file">bi_logistics_report.logistic_pdf_report</field>
        <field name="print_report_name">"Logistics Sales Report"</field>
        <field name="paperformat_id" ref="logistics_paperformat" />
    </record>

    <record id="action_scheduler_export_report" model="ir.actions.report">
        <field name="name">Logistics Sales Report</field>
        <field name="model">logistics.wizard</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">bi_logistics_report.export_scheduler_report_xlsx</field>
        <field name="report_file">bi_logistics_report.export_scheduler_report_xlsx</field>
        <field name="print_report_name">"Logistics Sales Report"</field>
    </record>


</odoo>
