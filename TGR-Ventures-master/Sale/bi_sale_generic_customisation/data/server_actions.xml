<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="po_sold_qty_action_server" model="ir.actions.server">
        <field name="name">Compute Sold Quantity</field>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="binding_model_id" ref="sale.model_sale_order"/>
        <field name="state">code</field>
        <field name="code">records._compute_sold_qty()</field>
    </record>

    <record id="sale_order_margin_action" model="ir.actions.server">
        <field name="name">Margin</field>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="binding_model_id" ref="sale.model_sale_order"/>
        <field name="state">code</field>
        <field name="code">records._action_margin_sale_order()</field>
    </record>


</odoo>
