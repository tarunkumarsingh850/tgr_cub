<odoo>
    <data noupdate="1">
      <record id="action_product_stock_scheduler" model="ir.cron">
        <field name="name">Pending Discontinued</field>
        <field name="model_id" ref="product.model_product_template"/>
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="code">model.action_trigger_pending_discontinued()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority">1</field>
      </record>

       <record id="action_back_in_stock_scheduler" model="ir.cron">
        <field name="name">Back in stock</field>
        <field name="model_id" ref="product.model_product_template"/>
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="code">model.action_trigger_back_in_stock()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority">1</field>
      </record>

       <record id="stock_update_csv_file_transfer_cron" model="ir.cron">
        <field name="name">Stock update csv file transfer</field>
        <field name="model_id" ref="product.model_product_product"/>
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="code">model.get_stock_update_csv_file_transfer()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="priority">1</field>
      </record>

       <record id="stock_update_free_qty_csv_cron" model="ir.cron">
        <field name="name">Stock update free qty csv</field>
        <field name="model_id" ref="product.model_product_product"/>
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="code">model.get_free_stock_update_csv_file_transfer()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="priority">1</field>
      </record>

      <record id="uk_stock_update_cron" model="ir.cron">
        <field name="name">UK Stock Update</field>
        <field name="model_id" ref="stock.model_stock_quant"/>
        <field name="state">code</field>
        <field name="code">model.update_uk_stock_cron()</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
      </record>

      <record id="eu_stock_update_cron" model="ir.cron">
        <field name="name">EU Stock Update</field>
        <field name="model_id" ref="stock.model_stock_quant"/>
        <field name="state">code</field>
        <field name="code">model.update_eu_stock_cron()</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
      </record>

      <record id="ws_stock_update_cron" model="ir.cron">
        <field name="name">Wholesale Stock Update</field>
        <field name="model_id" ref="stock.model_stock_quant"/>
        <field name="state">code</field>
        <field name="code">model.update_ws_stock_cron()</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
      </record>

      <record id="product_price_update_cron" model="ir.cron">
        <field name="name">Product Price Update</field>
        <field name="model_id" ref="product.model_product_template"/>
        <field name="state">code</field>
        <field name="code">model.update_product_price_cron(magento_website_id=1)</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
        <field name="active">0</field>
      </record>

      <record id="brand_import_cron" model="ir.cron">
        <field name="name">Brand Import</field>
        <field name="model_id" ref="bi_inventory_generic_customisation.model_product_breeder"/>
        <field name="state">code</field>
        <field name="code">model._sync_product_breeder_cron(instance=1)</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
        <field name="active">0</field>
      </record>

      <record id="product_pricelist_check" model="ir.cron">
        <field name="name">Product Pricelist Check</field>
        <field name="model_id" ref="product.model_product_pricelist"/>
        <field name="state">code</field>
        <field name="code">model._action_product_pricelist_check()</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
        <field name="active">1</field>
      </record>


    </data>
</odoo>
