    <record id="action_export_simple_template" model="ir.actions.report">
        <field name="name">Export Simple Product Template</field>
        <field name="model">product.website.export.import</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">bi_export_product_enable_website.export_simple_product</field>
        <field name="report_file">bi_export_product_enable_website.export_simple_product</field>
        <field name="print_report_name">"Export Simple Product Template"</field>
    </record>
</odoo>
