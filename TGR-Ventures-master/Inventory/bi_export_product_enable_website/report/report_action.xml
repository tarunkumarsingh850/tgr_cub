<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_export_template" model="ir.actions.report">
        <field name="name">Export Template</field>
        <field name="model">product.template</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">bi_export_product_enable_website.export_product_xlsx</field>
        <field name="report_file">bi_export_product_enable_website.export_product_xlsx</field>
        <field name="print_report_name">"Export Product Website Template"</field>
