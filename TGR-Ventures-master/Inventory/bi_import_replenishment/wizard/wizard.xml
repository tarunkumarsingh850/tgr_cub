<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="import_replenishment_view_form" model="ir.ui.view">
            <field name="name">import.replenishment.view.form</field>
            <field name="model">import.replenishment</field>
            <field name="arch" type="xml">
                <form string="Replenishment Import">
                    <group>

                        <group>
                            <field name="excel_file"/>
                        </group>
                        <group>
                            <button string="Download Template New" name="export_template" type="object"/>
                        </group>
                    </group>
                    <footer>
                        <button name="import_xls" string="Import Excel" type="object" class="btn-primary" />
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="import_replenishment_action" model="ir.actions.act_window">
        <field name="name">Replenishment Import</field>
        <field name="res_model">import.replenishment</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        </record>

    </data>
</odoo>
