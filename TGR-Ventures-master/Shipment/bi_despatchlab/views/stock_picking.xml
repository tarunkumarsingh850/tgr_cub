<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_picking_inherit_view_form" model="ir.ui.view">
        <field name="name">stock.picking.inherit.view.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_type']" position="before">
                <field name="diamond_carrier" readonly="1"/>
                <field name="diamond_shipment_id" readonly="1"/>
            </xpath>
        </field>
    </record>

</odoo>
