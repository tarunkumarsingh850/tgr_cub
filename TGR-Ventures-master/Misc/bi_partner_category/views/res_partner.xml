<odoo>
    <data>
      <record model="ir.ui.view" id="res_partner_view_inherit_category_form">
          <field name="name">res.partner.view.inherit.form</field>
          <field name="model">res.partner</field>
          <field name="inherit_id" ref="base.view_partner_form"/>
          <field name="arch" type="xml">
            <xpath expr="//form/sheet/notebook" position="inside">
                <page string="Documents">
                    <field name="res_partner_line_ids">
                        <tree string="Partner Category" editable="bottom">
                            <field name="partner_category_id"/>
                            <field name="start_date_"/>
                            <field name="end_date"/>
                            <field name="attachment_id" widget="many2many_binary"/>
                            <field name="reminder"/>
                        </tree>
                    </field>
                </page>
            </xpath>
            </field>
        </record>
    </data>
</odoo>
