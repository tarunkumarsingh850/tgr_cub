<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="One2ManySelectable" groups="one2many_selection.group_user_check">
        <div>
            <!-- Demo Button that will add 'delete' button above one2many selection but if you use widget="one2many_selectable" -->
            <t t-if="!widget.get('effective_readonly')">
                <button style="margin-left: 1%;margin-top: 1%;margin-bottom: 1%;display:none;" class="button_create_po_agreement btn btn-primary oe_highlight"
